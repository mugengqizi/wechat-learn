#outer {
  padding: 15rpx;
}
#outer view{
  width: 720rpx;
  height: 58rpx;
  background-color: #EEEEEE;
  /* 圆角设置 */
  border-radius: 12rpx;
  text-align: center;
  line-height: 53rpx;
}
#outer image {
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
}
#outer  text {
  color: #a9a9a9;
  font-size: 28rpx;
  padding-left: 10rpx;
}
#santu{
  display: flex;
}
#santu > view{
  border-radius: 10rpx;
  margin: 10rpx 10rpx;
  height: 100rpx;
  background-color: #88d1f9;
  display: flex;
  flex: 1;
  align-items: center;
  justify-content: center;
}
#santu image{
  width: 45rpx;
  height: 45rpx;
  vertical-align: middle;
}
#santu text{
  color: white;
  font-weight: bold;
}
