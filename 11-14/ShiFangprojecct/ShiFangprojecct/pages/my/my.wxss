.one{
  background-color: #2BA2FE;
  height: 220rpx;
  margin: 30rpx 10rpx 30rpx 10rpx;
  padding-top: 60rpx;
  border-radius: 30rpx;
}
.one view{
  color: white;
  text-align: center;
  font-size: 30rpx;
  margin-bottom: 30rpx;
}
.one button{
  font-size: 22rpx;
  background-color: blueviolet;
  width: 200rpx;
  border-radius: 50rpx;
}
.up{
  /* display: flex; */
  margin: 0px 10px;
  padding-left: 30rpx;
  padding-right: 50rpx;
  padding-bottom: 20rpx;
  padding-top: 20rpx;
  background-color: white;
}
.up > view{
  display: flex;
  margin: 20rpx 0rpx;
  justify-content: space-between;
}
.up image{
  width: 40rpx;
  height: 40rpx;
}

